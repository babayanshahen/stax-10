<div itemscope itemtype="http://schema.org/Product" data-section-id="{{ section.id }}" data-section-type="product" data-history-state>{% render 'preorder-now-collection-item', product: product %}
    {% comment %}
        Get first variant, or deep linked one
    {% endcomment %}
    {% assign current_variant = product.selected_or_first_available_variant %}
    {% comment %}
        Get first image
    {% endcomment %}
    {% assign featured_image = current_variant.featured_image | default: product.featured_image %}
    {% comment %}
        Get the text and link for the back button
    {% endcomment %}
    {% assign back_url = '/' %}
    {% assign back_text = 'products.product.back' | t %}
    {% if collection %}
        {% assign back_url = collection.url %}
        {% assign back_text = collection.title %}
    {% endif %}
    <div itemprop="brand" itemtype="http://schema.org/Thing" itemscope>
        <meta itemprop="name" content="{{ shop.name }}"/>
    </div>
    <meta itemprop="sku" content="{{ current_variant.sku }}"/>
    <meta itemprop="image" content="{{ featured_image | img_url: 'grande' }}">
        <meta itemprop="name" content="{{ product.title }}{% if product.variants.size > 1 and product.selected_variant %} - {{ current_variant.title }}{% endif %}">
            <div class="product grid grid--uniform grid--no-gutters" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
                <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
                    {% assign first_image = featured_image %}
                    {% if product.images.size > 1 and section.settings.skip_first_product_image and first_image == product.featured_image %}
                        {% assign first_image = product.images[1] %}
                    {% endif %}
                    {% comment %}
                        Set variables for product image grid
                    {% endcomment %}
                    {% if section.settings.product_image_type == 'portrait' %}
                        {% if product.images.size == 1 %}
                            {% assign grid_item_width = '' %}
                        {% elsif product.images.size == 2 and section.settings.skip_first_product_image %}
                            {% assign grid_item_width = '' %}
                        {% else %}
                            {% assign grid_item_width = 'medium-up--one-half' %}
                        {% endif %}
                        {% assign is_portrait_image = true %}
                    {% else %}
                        {% assign grid_item_width = '' %}
                    {% endif %}
                    <style>
                        .product-col ul.support__images li {
                            width: 32%;
                            margin-right: 0.875%;
                            float: left;
                            list-style-type: none;
                            margin-bottom: 0.875%;
                        }
                         #ProductSelect-product-template-cap {
                            display: block !important;
                            margin-bottom: 10px;
                        }
                        .product-col ul.support__images.desktop--images {
                            display: block;
                        }
                        img {
                            max-width: 100%;
                            max-height: 100%;
                            border: none;
                            display: block;
                        }
                        .product-col #featureimage .featured-image {
                            max-height: inherit !important;
                        }
                        @media only screen and(min-width: 750px) {
                            .product-col #featureimage .featured-image {
                                display: block;
                            }
                        }
                        @media only screen and(min-width: 750px) {
                            .container.page-full-width {
                                max-width: 95%;
                                margin: 0 auto;
                            }
                            @media only screen and(min-width: 750px) {
                                .product__breadcrumb {
                                    margin: 10px 0 20px;
                                    text-align: left;
                                    display: block;
                                }
                            }
                            .product-details h2 {
                                margin-top: 40px;
                            }
                            /*       .product-col .product-details {
        margin: 0 auto;
        width: 100%;
        text-align: center;
        padding: 0 20px;
      }
 */
                            .product__breadcrumb {
                                letter-spacing: 0.025rem;
                                font-family: "Montserrat", Helvetica, Arial, sans-serif;
                                font-size: 0.7rem;
                                color: #111;
                                text-transform: uppercase;
                            }
                            #AddToCartForm-product-template {
                                margin: 0 !important;
                            }
                            #ProductSelect-product-template-slides {
                                display: block !important;
                                margin-top: 1rem !important;
                            }
                            .btn.btn-primary.button,
                            .product-single__description .rte {
                                width: 100%;
                            }
                            .social-sharing a {
                                display: flow-root;
                            }
                        </style>
                    {% if product.title != 'STAX. SLIDES WHITE/BLACK (UNISEX)' and  product.title != 'JH SLIDES SNAKE SKIN - BLACK' and  product.title != 'JH SLIDES SNAKE SKIN - PINK' %}
                            <style>/*       #AddToCartForm-product-template .selector-wrapper{
        display:none !important;
      } */</style>
                        {% else: %}
                        <style>
                            #AddToCart-product-template {
                                display: block !important;
                                margin: 0 !important;
                            }
                            #ProductSelect-product-template- {
                                display: block !important;
                                margin-top: 1rem !important;
                                width: 100%;
                            }
                            .btn.btn-primary.button,
                            .product-single__description .rte {
                                width: 100%;
                            }
                            .social-sharing a {
                                display: flow-root;
                            }
                        }
                        </style>
                    {% endif %}
                    <div id="cache-recomended"></div>
                    {% if product.images.size > 0 %}
                        <div class="container page-full-width">
                            <div class='grid equal'>
                                <div class="product__photo grid__item">
                                    <div
                                        class="grid__item small--one-whole product-col medium-up--seven-twelfths" style="position: relative;">
                                        <!--             <div class="button-prev-nex">
                                                        <img src="https://cdn.shopify.com/s/files/1/0854/5504/files/back.png?5969" width="50" height="50" id='arrow-prev'>
                                                        <img src="https://cdn.shopify.com/s/files/1/0854/5504/files/next.png?6037" width="50" height="50" id='arrow-next'>  
                                                      </div> -->
                                        <div id="artur" style="height: auto;" class="support__images desktop--images product_slick">
                                            <nav class="product__breadcrumb" role="navigation" aria-label="breadcrumbs">
                                                <span>
                                                    <a href="/" title="">Home</a>
                                                </span>
                                                <span aria-hidden="true">—</span>
                                                {% if  collection.title !=  null %}
                                                    <a href="{{ collection.url }}" title="">{{ collection.title }}</a>
                                                    <span aria-hidden="true">—</span>
                                                {% endif %}
                                                <span class="bn_new_color_style">{{ product.title }}</span>
                                            </nav>
                                            {% assign first_img = '' %}
                                            {% assign first_img_url = '' %}
                                            {% for image in product.images %}
                                                {% assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                                                {% if  forloop.index  == 1 %}
                                                    {% assign first_img = image %}
                                                    {% assign first_img_url = img_url %}
                                                {% endif %}
                                                <div style="width:49%">
                                                    <img class="grid-view-item__image lazyload {{ forloop.index }}" data-src="{{ image | img_url: '800x' }}" data-widths="[360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048, 4472]" data-aspectratio="{{ image.aspect_ratio }}" data-sizes="auto" src="" alt="{{ image.alt | escape }}"></div>
                                                {% endfor %}
                                            </div>
                                             <div class="vip-list-social-btns">
                                                {% comment %}
                                                    <div class="vip-list-prod">
                                                        <div
                                                            class="vip_list_main">
                                                            <!-- <h3>VIP SIGN UP EXCLUSIVE DISCOUNTS, EARLY RELEASES & RESTOCKS</h3> -->
                                                            <div class="vip_form_content klaviyo-form-PGsuNd"></div>
                                                        </div>
                                                    </div>
                                                {% endcomment %}
                                                <div class="social-sharin-tex-align">
                                                    {% if section.settings.social_sharing_products %}
                                                        {% include 'social-sharing', share_title: product.title, share_permalink: product.url, share_image: product %}
                                                    {% endif %}
                                                </div>
                                            </div>
                                            {% comment %}
                                                {% include 'bold-sales-text' %}
                                                                            {% include 'shappify-sales-clock' %}
                                            {% endcomment %}
                                            
                                        </div>
                                        <noscript>
                                            <img class="product__photo--variant" src="{{ first_image | img_url: '2048x2048' }}" alt="{{ image.alt | escape }}"></noscript>
                                            <div class="grid__item medium-up--five-twelfths product-col">
                                                <div class="sticky-wrapper sticky">
                                                    {% if product.metafields.title.meta_title != blank %}
                                                        <h1>{{ product.metafields.title.meta_title }}</h1>
                                                    {% else %}
                                                        <h1 class="prod-title-desktop">{{ product.title }}</h1>
                                                    {% endif %}
                                                    <div class="product-details">
                                                        {% if section.settings.show_vendor %}
                                                            <p class="product-item__vendor small--text-center">{{ product.vendor }}</p>
                                                        {% endif %}
                                                        <!--  <h2 itemprop="name">{{ product.title }}</h2> -->
                                                        {% comment %}Start automatically added Judge.me widget{% endcomment %}
                                                        {% include 'judgeme_widgets', widget_type: 'judgeme_preview_badge', concierge_install: true %}
                                                        {% comment %}End automatically added Judge.me widget{% endcomment %}
                                                        <span>{{ collection.title }}</span>
                                                        <link itemprop="availability" href="http://schema.org/{% if current_variant.available %}InStock{% else %}OutOfStock{% endif %}">
                                                            {%- capture 'form_id' -%}AddToCartForm-{{ section.id }}
                                                            {%- endcapture -%}
                                                            {%- capture 'form_class' -%}
                                                                product__form--add-to-cart
                                                                {% if section.settings.product_form_width == 'large' %}
                                                                    product__form--full-width
                                                                {% endif %}
                                                            {%- endcapture -%}
