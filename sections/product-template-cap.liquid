                                                                                                            <div class="video_and_vip">
                                                                                                                <div>
                                                                                                                    <video id="stax-video" src="{{ product.metafields.videos.video_1_content }}" controls playsinline style="display: inline-block;" width='50%'></video>
                                                                                                                </div>
                                                                                                            </div>
                                                                                                        {% endif %} {% endcomment %}
                                                                                                        <!-- Autoinstall code -->
