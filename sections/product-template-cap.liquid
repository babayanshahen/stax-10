                                                                                                                    <iframe src="https://stax.com.au/pages/bn_shipping" height="200" width="100%" title="" frameBorder="0"></iframe>
                                                                                                   
                                                                                                    {% comment %} <a class="btn btn-primary button" href="https://stax.com.au/pages/shipping">More Details</a> {% endcomment %}
                                                                                                </div>
                                                                                            </li>
                                                                                            <li class="desc-item" data-id="ret-body">
                                                                                                <h3>RETURNS</h3>
                                                                                                <div id="ret-body" class="desc-all-body">
                                                                                                                     <iframe src="https://stax.com.au/pages/returns" height="200" width="100%" title="" frameBorder="0"></iframe>
                                                                                                   
                                                                                                    {% comment %} <a class="btn btn-primary button" href="https://stax.com.au/pages/return-policy">More Details</a> {% endcomment %}
                                                                                                </div>
                                                                                            </li>
                                                                                        </ul>
                                                                                    </div>
                                                                                </div>
                                                                                {% if section.settings.show_quantity_selector %}
                                                                                    <div class="product__quantity quantity-wrapper">
                                                                                        <label for="Quantity" class="product__quantity-label">{{ 'products.product.quantity' | t }}</label>
                                                                                        <input type="number" id="Quantity" class="quantity-input" name="quantity" value="1" min="1"></div>
                                                                                    {% endif %}
                                                                                </div>
