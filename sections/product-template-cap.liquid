                                                                                    <div class="video_and_vip">
                                                                                        <div style="width:49%">
                                                                                            <video id="stax-video" src="{{ product.metafields.videos.video_1_content }}" controls playsinline style="display: inline-block;" width='100%'></video>
                                                                                        </div>
