                                                                                                <img class="lazyload" data-src="https://cdn.shopify.com/s/files/1/0854/5504/files/BEST_BLACK_TIGHT_S-min_compressed.png?v=1593095890" alt="best black tights image"></a>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                {% endform %}
                                                                                 {% include 'swym-product-view', product: product %}
                                                                                            <div class="custom-swym-btn-container"> 
