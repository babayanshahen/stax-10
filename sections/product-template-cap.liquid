                                                                <div style="display:none" test='asdasdasd'>
                                                                    {{ product.metafields.colors.product_color }}
                                                                </div>
                                                                {% if product.metafields.colors.product_color != blank %}
                                                                    {% assign productColors = product.metafields.colors.product_color | split: ',' %}
