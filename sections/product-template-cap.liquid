                                                                        <div class="product_color_content" style="margin-bottom: 10px;">
                                                                            <h3>Color</h3>
                                                                            <span id="product_color_item"></span>
                                                                            <br>
                                                                                {% for color in productColors %}
                                                                                    {% assign splitedColor = color | split: ':' %}
                                                                                    <div class="check_color_border">
                                                                                        <a href="{{ splitedColor[1] | strip }}">
                                                                                            <div id="check_color_circle_item" style="background-color:#{{ splitedColor[0] |  strip }}"></div>
                                                                                        </a>
                                                                                    </div>
                                                                                {% endfor %}
                                                                            </div>
                                                                        </div>
                                                                    {% endif %}
                                                                {% endif %}
                                                                {% comment %} <button id="myBtn">Open Modal</button> {% endcomment %}
                                                                {% form 'product', product, id:form_id, class:form_class %}
                                                                    {% comment %}
                                                                        Select element hidden when JS initializes. Safeguard for JS-disabled.
                                                                    {% endcomment %}
                                                                    <select name="id" id="ProductSelect-{{ section.id }}">
                                                                        {% for variant in product.variants %}
                                                                            <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} {% unless variant.available %} disabled="disabled" {% endunless %} value="{{ variant.id }}" data-sku="{{ variant.sku }}">{{ variant.title }}
                                                                                -
                                                                                {% if variant.available %}
                                                                                    {{ variant.price | money_with_currency }}
                                                                                {% else %}
                                                                                    {{ 'products.product.sold_out' | t }}
                                                                                {% endif %}
                                                                            </option>
                                                                        {% endfor %}
                                                                    </select>
                                                                     
                                                                    {% assign price = current_variant.price | money_without_trailing_zeros %}
                                                                    <p class="product__price">
                                                                        <meta itemprop="priceCurrency" content="{{ shop.currency }}">
                                                                            <meta itemprop="price" content="{{ current_variant.price | divided_by: 100.00 }}">
                                                                                {% if product.compare_at_price_max > product.price %}
                                                                                    <span class="visually-hidden">{{ 'products.general.regular_price' | t }}</span>
                                                                                    <span class="product__price--reg">{{ current_variant.compare_at_price | money_without_trailing_zeros }}</span>
                                                                                    <span class="product__price--sale">{{ 'products.general.now_price_html' | t: price: price }}</span>
                                                                                {% else %}
                                                                                    <span class="visually-hidden">{{ 'products.general.regular_price' | t }}</span>
                                                                                    <span class="product__price--reg js-price">
                                                                                        {{ price }}
                                                                                    </span>
                                                                                {% endif %}
                                                                            </p>
                                                                            <div class="bn-for-product-payment-info">
                                                                              <ul class="bn_new_color_style">
                                                                                <li><i class="material-icons">brightness_1</i>
                                                                                  {%- assign latitudepay_price = current_variant.price | divided_by: 10 | money -%}
                                                                                  <div  class="custom-latitudepay" style="font-weight:300;margin-bottom:15px;font-size:14px;">
                                                                                      <a class="custom-latitude-link" href="https://www.latitudepay.com" target="_blank" style="display: flex;">
                                                                                          <img style="width:115px;" src="https://d5gx0tid0xr61.cloudfront.net/latitudepay_250px.png"/>
                                                                                          {% comment %}
                                                                                              <div class="new-budge-latitude">			
                                                                                                                  RECOMMENDED	
                                                                                                                  </div>
                                                                                          {% endcomment %}
                                                                                      </a>
                                                                                      or 10 interest free payments from
                                                                                      <b class="bn_new_color_style">{{ latitudepay_price }}</b>
                                                                                      {% if current_variant.price < 2000 %}
                                                                                          on orders over $20
                                                                                      {% endif %}
                                                                                      with
                                                                                      <span style="position:relative;top:3px;white-space:nowrap;display:inline-block;">
                                                                                          <a style="position:relative;top:-6px;text-decoration:underline;font-size:11px;cursor:pointer;" target="_blank" href="https://www.latitudepay.com/how-it-works/">What's this?</a>
                                                                                      </span>
                                                                                  </div>
                                                                                </li>
                                                                                <li><i class="material-icons">brightness_1</i>
                                                                                  <div class="zippay">
                                                                                      <p class="bn_new_color_style">Zippay</p>
                                                                                      <img src="https://cdn.shopify.com/s/files/1/0854/5504/files/zippay.png?498" alt="zippay" width="50" height="20"></div>
                                                                                </li>
                                                                                <li><i class="material-icons">brightness_1</i>
                                                                                      <div class="laybuy-bn">
                                                                                          <div class='lay-buy-section'></div>
                                                                                          <h4 class="bn_new_color_style">NEW ZEALAND & UK</h4>
                                                                                      </div>
                                                                                    </li>  
                                                                                </ul>
                                                                              </div>
                                                                                {% if product.available %}
                                                                                    {% if product.tags contains  'bn_pre_order_button' %}
                                                                                        <button data-toggle="modal" data-target=".bd-example-modal-lg" onload="this.stopPropagation()" type="button" name="add" class="btn  btn--add-to-cart{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}">
                                                                                            <span class="">Pre Order</span>
                                                                                        </button>
                                                                                        <div class="preorder-description">Please note this item is on pre order
                                                                                        </div>
                                                                                    {% else %}
                                                                                        {% if product.tags contains 'new_preorder_style' %}
                                                                                            <!--                    <p class="weeks-4-6" style="display: none;">SHIPPING IN 4-6 weeks</p> -->
                                                                                            {% if product.tags contains 'bn-pre-sale' and product.tags contains 'bn-pre-shipping-6-8' %}
                                                                                                <p class="weeks-4-6" style="display: none;">SHIPPING IN 6-8 weeks
                                                                                                </p>
                                                                                            {% elsif product.tags contains 'bn-pre-sale' and product.tags contains 'bn-pre-shipping-7-days' %}
                                                                                                <p class="weeks-4-6" style="display: none;">SHIPPING IN 7 days</p>
                                                                                            {% elsif product.tags contains 'bn-pre-sale' and product.tags contains 'bn-pre-shipping-4-6' %}
                                                                                                <p class="weeks-4-6" style="display: none;">SHIPPING IN 4-6 weeks</p>
                                                                                            {% endif %}
                                                                                            <div class="buy-now-preOrder-btns">
                                                                                                <button type="button" class="only-btn-by-shop  btn buy-now-btn btn--add-to-cart{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}" name="add">Buy Now</button>
                                                                                                <button style="display: none;" val="pre-order" type="button" data-toggle="modal" data-target=".bd-example-modal-lg" class="only-btn-by-shop btn btn--add-to-cart{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}" name="add">Pre Order</button>
                                                                                                <button type="button" data-toggle="modal" data-target=".bd-example-modal-lg" class="only-btn-by-shop btn preOrder-btn">Pre Order</button>
                                                                                            </div>
                                                                                        {% else %}
                                                                                            <button id="AddToCart-{{ section.id }}" type="button" name="add" class="btn btn--add-to-cart{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}" disabled="true">
                                                                                                <span class="btn__text">{{ 'products.product.add_to_cart' | t }}</span>
                                                                                            </button>
                                                                                        {% endif %}
                                                                                    {% endif %}
                                                                                {% else %}
                                                                                    <button type="button" class="btn btn--add-to-cart btn--disabled{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}" disabled="disabled">
                                                                                        {{ 'products.product.sold_out' | t }}
                                                                                    </button>
                                                                                {% endif %}
                                                                                {% if section.settings.enable_payment_button %}
                                                                                    <!--                     {{ form | payment_button }} -->
                                                                                {% endif %}
                                                                                <div class="modal popup_collection fade">
                                                                                    <div class="popup-col-content">
