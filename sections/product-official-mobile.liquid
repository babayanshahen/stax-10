<div itemscope itemtype="http://schema.org/Product" data-section-id="{{ section.id }}" data-section-type="product" data-history-state>{% render 'preorder-now-collection-item', product: product %}
    {% comment %}
        Get first variant, or deep linked one
    {% endcomment %}
    {% assign current_variant = product.selected_or_first_available_variant %}
    {% comment %}
        Get first image
    {% endcomment %}
    {% assign featured_image = current_variant.featured_image | default: product.featured_image %}
    {% comment %}
        Get the text and link for the back button
    {% endcomment %}
    {% assign back_url = '/' %}
    {% assign back_text = 'products.product.back' | t %}
    {% if collection %}
        {% assign back_url = collection.url %}
        {% assign back_text = collection.title %}
    {% endif %}
    <div itemprop="brand" itemtype="http://schema.org/Thing" itemscope>
        <meta itemprop="name" content="{{ shop.name }}"/>
    </div>
    <meta itemprop="sku" content="{{ current_variant.sku }}"/>
    <meta itemprop="image" content="{{ featured_image | img_url: 'grande' }}">
        <meta itemprop="name" content="{{ product.title }}{% if product.variants.size > 1 and product.selected_variant %} - {{ current_variant.title }}{% endif %}">
            <div class="product grid grid--uniform grid--no-gutters" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
                <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
                    {% assign first_image = featured_image %}
                    {% if product.images.size > 1 and section.settings.skip_first_product_image and first_image == product.featured_image %}
                        {% assign first_image = product.images[1] %}
                    {% endif %}
                    {% comment %}
                        Set variables for product image grid
                    {% endcomment %}
                    {% if section.settings.product_image_type == 'portrait' %}
                        {% if product.images.size == 1 %}
                            {% assign grid_item_width = '' %}
                        {% elsif product.images.size == 2 and section.settings.skip_first_product_image %}
                            {% assign grid_item_width = '' %}
                        {% else %}
                            {% assign grid_item_width = 'medium-up--one-half' %}
                        {% endif %}
                        {% assign is_portrait_image = true %}
                    {% else %}
                        {% assign grid_item_width = '' %}
                    {% endif %}
                    <style>
                        .product-col ul.support__images li {
                            width: 32%;
                            margin-right: 0.875%;
                            float: left;
                            list-style-type: none;
                            margin-bottom: 0.875%;
                        }
                        .product-col ul.support__images.desktop--images {
                            display: block;
                        }
                        img {
                            max-width: 100%;
                            max-height: 100%;
                            border: none;
                            display: block;
                        }
                        .product-col #featureimage .featured-image {
                            max-height: inherit !important;
                        }
                        @media only screen and(min-width: 750px) {
                            .product-col #featureimage .featured-image {
                                display: block;
                            }
                        }
                        @media only screen and(min-width: 750px) {
                            .container.page-full-width {
                                max-width: 95%;
                                margin: 0 auto;
                            }
                            @media only screen and(min-width: 750px) {
                                .product__breadcrumb {
                                    margin: 10px 0 20px;
                                    text-align: left;
                                    display: block;
                                }
                            }
                            .product-details h2 {
                                margin-top: 40px;
                            }
                            /*       .product-col .product-details {
        margin: 0 auto;
        width: 100%;
        text-align: center;
        padding: 0 20px;
      }
 */
                            .product__breadcrumb {
                                letter-spacing: 0.025rem;
                                font-family: "Montserrat", Helvetica, Arial, sans-serif;
                                font-size: 0.7rem;
                                color: #111;
                                text-transform: uppercase;
                            }
                            #AddToCartForm-product-template {
                                margin: 0 !important;
                            }
                            #ProductSelect-product-template-slides {
                                display: block !important;
                                margin-top: 1rem !important;
                            }
                            .btn.btn-primary.button,
                            .product-single__description .rte {
                                width: 100%;
                            }
                            .social-sharing a {
                                display: flow-root;
                            }
                        </style>
                    {% if product.title != 'STAX. SLIDES WHITE/BLACK (UNISEX)' and  product.title != 'JH SLIDES SNAKE SKIN - BLACK' and  product.title != 'JH SLIDES SNAKE SKIN - PINK' %}
                            <style>/*       #AddToCartForm-product-template .selector-wrapper{
        display:none !important;
      } */</style>
                        {% else: %}
                        <style>
                            #AddToCart-product-template {
                                display: block !important;
                                margin: 0 !important;
                            }
                            #ProductSelect-product-template- {
                                display: block !important;
                                margin-top: 1rem !important;
                                width: 100%;
                            }
                            .btn.btn-primary.button,
                            .product-single__description .rte {
                                width: 100%;
                            }
                            .social-sharing a {
                                display: flow-root;
                            }
                        }
                        </style>
                    {% endif %}
                    <div id="cache-recomended"></div>
                    {% if product.images.size > 0 %}
                        <div class="container page-full-width">
                            <div class='grid equal'>
                                <div class="product__photo grid__item">
                                    <div
                                        class="grid__item small--one-whole product-col medium-up--seven-twelfths" style="position: relative;">
                                        <!--             <div class="button-prev-nex">
                                                        <img src="https://cdn.shopify.com/s/files/1/0854/5504/files/back.png?5969" width="50" height="50" id='arrow-prev'>
                                                        <img src="https://cdn.shopify.com/s/files/1/0854/5504/files/next.png?6037" width="50" height="50" id='arrow-next'>  
                                                      </div> -->
                                        <div class="product-image-container-mobile">
                                            {% if product.tags contains "bn_pre_badge" %}
                                                <div class="bn-badge-sale">
                                                    <p class="preTitle">Pre Sale</p>
                                                    <p class="preMessage">SHIPPING IN 6-8 WEEKS</p>
                                                </div>
                                            {% endif %}
                                            <div id="artur" style="height: auto;" class="support__images desktop--images product_slick">
                                                <nav class="product__breadcrumb" role="navigation" aria-label="breadcrumbs">
                                                    <span>
                                                        <a href="/" title="">Home</a>
                                                    </span>
                                                    <span aria-hidden="true">—</span>
                                                    {% if  collection.title !=  null %}
                                                        <a href="{{ collection.url }}" title="">{{ collection.title }}</a>
                                                        <span aria-hidden="true">—</span>
                                                    {% endif %}
                                                    <span>{{ product.title }}</span>
                                                </nav>
                                                {% assign first_img = '' %}
                                                {% assign first_img_url = '' %}
                                                {% for image in product.images %}
                                                    {% assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                                                    {% if  forloop.index  == 1 %}
                                                        {% assign first_img = image %}
                                                        {% assign first_img_url = img_url %}
                                                    {% endif %}
                                                    <div style="width:49%">
                                                        <img class="grid-view-item__image lazyload {{ forloop.index }}" data-src="{{ image | img_url: '800x' }}" data-widths="[360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048, 4472]" data-aspectratio="{{ image.aspect_ratio }}" data-sizes="auto" src="" alt="{{ image.alt | escape }}"></div>
                                                    {% endfor %}
                                                </div>
                                            </div>
                                            {% if product.metafields.title.meta_title != blank %}
                                                <h1>{{ product.metafields.title.meta_title }}</h1>
                                            {% else %}
                                                <h1 class="prod-title-desktop">{{ product.title }}</h1>
                                            {% endif %}
                                            <div class="product-details">
                                                {% if section.settings.show_vendor %}
                                                    <p class="product-item__vendor small--text-center">{{ product.vendor }}</p>
                                                {% endif %}
                                                <!--  <h2 itemprop="name">{{ product.title }}</h2> -->
                                                {% comment %}Start automatically added Judge.me widget{% endcomment %}
                                                {% include 'judgeme_widgets', widget_type: 'judgeme_preview_badge', concierge_install: true %}
                                                {% comment %}End automatically added Judge.me widget{% endcomment %}
                                                <span>{{ collection.title }}</span>
                                                <link itemprop="availability" href="http://schema.org/{% if current_variant.available %}InStock{% else %}OutOfStock{% endif %}">
                                                    {%- capture 'form_id' -%}AddToCartForm-{{ section.id }}
                                                    {%- endcapture -%}
                                                    {%- capture 'form_class' -%}
                                                        product__form--add-to-cart
                                                        {% if section.settings.product_form_width == 'large' %}
                                                            product__form--full-width
                                                        {% endif %}
                                                    {%- endcapture -%}
                                                    {% if product.title != 'STAX. SLIDES WHITE/BLACK (UNISEX)' and  product.title != 'JH SLIDES SNAKE SKIN - BLACK' and  product.title != 'JH SLIDES SNAKE SKIN - PINK' %}
                                                        <div style="display:none">
                                                            {{ product.metafields.colors.product_color }}
                                                        </div>
                                                        {% if product.metafields.colors.product_color != blank %}
                                                            {% assign productColors = product.metafields.colors.product_color | split: ',' %}
                                                            <div id="product_color">
                                                                <div class="product_color_content" style="margin-bottom: 10px;">
                                                                    <h3>Color</h3>
                                                                    <span id="product_color_item"></span>
                                                                    <br>
                                                                        {% for color in productColors %}
                                                                            {% assign splitedColor = color | split: ':' %}
                                                                            <div class="check_color_border">
                                                                                <a href="{{ splitedColor[1] | strip }}">
                                                                                    <div id="check_color_circle_item" style="background-color:#{{ splitedColor[0] |  strip }}"></div>
                                                                                </a>
                                                                            </div>
                                                                        {% endfor %}
                                                                    </div>
                                                                </div>
                                                            {% endif %}
                                                            <div class="size-box-and-size-chart">
                                                                <div class="quickBuyButtons-product">
                                                                    {% for variant in product.variants %}
                                                                        {% if variant.available %}
                                                                            {% if variant.title == 'default cap' %}
                                                                                <div class="varwrapper three columns quickBuySizeContainerFour-prod ">
                                                                                    <li class="quickBuySizeInput-product" class="data-q-li-{{ forloop.index }}" data-q-li="{{ forloop.index }}">
                                                                                        <img style="width: 54px;position: relative;" src="https://cdn.shopify.com/s/files/1/0854/5504/files/Screenshot_5.jpg?5411"></li>
                                                                                    </div>
                                                                                {% endif %}
                                                                                {% if variant.title != 'Default Title' %}
                                                                                    <div class="{% if variant.title contains '/' %}bn-size-message prod-official-mobile {% endif %} varwrapper three columns quickBuySizeContainerFour-prod " data-variant-title="{{ variant.title }}">
                                                                                        <li class="quickBuySizeInput-product" class="data-q-li-{{ forloop.index }}" data-q-li="{{ forloop.index }}">
                                                                                            {% if variant.inventory_policy == "continue" %}
                                                                                                {% unless product.tags contains "bn_pre_important" %}
                                                                                                    {% if variant.inventory_quantity < 1 %}
                                                                                                        <div class="continue-pre-sale bn-custom-pre-sale">
                                                                                                            <div class="continue-pre-content">Pre Sale</div>
                                                                                                        </div>
                                                                                                    {% endif %}
                                                                                                {% endunless %}
                                                                                            {% endif %}
                                                                                            {% if variant.inventory_quantity > 0 and product.tags contains "bn_pre_important" %}
                                                                                                <div class="continue-pre-sale">
                                                                                                    <div class="continue-pre-content">Pre Sale</div>
                                                                                                </div>
                                                                                            {% endif %}
                                                                                            {% case variant.title %}
                                                                                                {% when 'xs' or 'XS' %}
                                                                                                    XS
                                                                                                {% when 'small' or 'S' %}
                                                                                                    S
                                                                                                {% when 'medium' or 'M' %}
                                                                                                    M
                                                                                                {% when 'large'  or 'L' %}
                                                                                                    L
                                                                                                {% when 'xl' or 'XL' %}
                                                                                                    XL
                                                                                                {% when '2xl' or  '2XL' %}
                                                                                                    2XL
                                                                                                {% when '3xl' or '3XL' %}
                                                                                                    3XL
                                                                                                {% when 'XS/S' %}
                                                                                                    WMNS-XS MENS-S
                                                                                                {% when 'S/M' %}
                                                                                                    WMNS-S MENS-M
                                                                                                {% when 'M/L' %}
                                                                                                    WMNS-M MENS-L
                                                                                                {% when 'L/XL' %}
                                                                                                    WMNS-L MENS-XL
                                                                                                {% when 'XL/2XL' %}
                                                                                                    WMNS-XL MENS-2XL
                                                                                                {% when '2XL/3XL' %}
                                                                                                    WMNS-2XL MENS-3XL
                                                                                            {% endcase %}
                                                                                        </li>
                                                                                    </div>
                                                                                    {% else: %}
                                                                                    {% form 'product', product, id:form_id, class:form_class %}
                                                                                        {% comment %}
                                                                                            Select element hidden when JS initializes. Safeguard for JS-disabled.
                                                                                        {% endcomment %}
                                                                                        <select name="id" id="ProductSelect-{{ section.id }}" style="display: none;">
                                                                                            {% for variant in product.variants %}
                                                                                                <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} {% unless variant.available %} disabled="disabled" {% endunless %} value="{{ variant.id }}" data-sku="{{ variant.sku }}">{{ variant.title }}
                                                                                                    -
                                                                                                    {% if variant.available %}
                                                                                                        {{ variant.price | money_with_currency }}
                                                                                                    {% else %}
                                                                                                        {{ 'products.product.sold_out' | t }}
                                                                                                    {% endif %}
                                                                                                </option>
                                                                                            {% endfor %}
                                                                                        </select>
                                                                                        {% assign price = current_variant.price | money_without_trailing_zeros %}
                                                                                        <p class="product__price">
                                                                                            <meta itemprop="priceCurrency" content="{{ shop.currency }}">
                                                                                                <meta itemprop="price" content="{{ current_variant.price | divided_by: 100.00 }}">
                                                                                                    {% if product.compare_at_price_max > product.price %}
                                                                                                        <span class="visually-hidden">{{ 'products.general.regular_price' | t }}</span>
                                                                                                        <span class="product__price--reg">{{ current_variant.compare_at_price | money_without_trailing_zeros }}</span>
                                                                                                        <span class="product__price--sale">{{ 'products.general.now_price_html' | t: price: price }}</span>
                                                                                                    {% else %}
                                                                                                        <span class="visually-hidden">{{ 'products.general.regular_price' | t }}</span>
                                                                                                        <span class="product__price--reg js-price">
                                                                                                            {{ price }}
                                                                                                        </span>
                                                                                                    {% endif %}
                                                                                                </p>
                                                                                                {% if product.available %}
                                                                                                    {% case product.tags %}
                                                                                                        {% when 'bn_pre_order_button' %}
                                                                                                            <button onload="this.stopPropagation()" type="submit" name="add" class="btn btn--add-to-cart{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}">
                                                                                                                <span class="">Pre Order</span>
                                                                                                            </button>
                                                                                                            <div class="preorder-description">Please note this item is on pre order
                                                                                                            </div>
                                                                                                        {% when 'new_preorder_style' %}
                                                                                                            <button>Test</button>
                                                                                                        {% else %}
                                                                                                            <button type="submit" name="add" class="btn btn--add-to-cart{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}">
                                                                                                                <span class="btn__text">{{ 'products.product.add_to_cart' | t }}</span>
                                                                                                            </button>
                                                                                                    {% endcase %}
                                                                                                {% else %}
                                                                                                    <button type="button" class="btn btn--add-to-cart btn--disabled{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}" disabled="disabled">
                                                                                                        {{ 'products.product.sold_out' | t }}
                                                                                                    </button>
                                                                                                {% endif %}
                                                                                                {% if section.settings.enable_payment_button %}
                                                                                                    {{ form | payment_button }}{% endif %}
                                                                                                {% endform %}
                                                                                            {% endif %}
                                                                                        {% else %}
                                                                                            {% if variant.title != 'Default Title' %}
                                                                                                <div class="varwrapper three columns quickBuySizeContainerFour-prod quickBuySizeContainerBorderRight  " title="Sign up for restock notifications!">
                                                                                                    <li class="quickBuySizeInput-product disabled strikethrough">
                                                                                                        {% case variant.title %}
                                                                                                            {% when 'xs' or 'XS' %}
                                                                                                                XS
                                                                                                            {% when 'small' or 'S' %}
                                                                                                                S
                                                                                                            {% when 'medium' or 'M' %}
                                                                                                                M
                                                                                                            {% when 'large'  or 'L' %}
                                                                                                                L
                                                                                                            {% when 'xl' or 'XL' %}
                                                                                                                XL
                                                                                                            {% when '2xl' or  '2XL' %}
                                                                                                                2XL
                                                                                                            {% when '3xl' or '3XL' %}
                                                                                                                3XL
                                                                                                            {% when 'XS/S' %}
                                                                                                                WMNS-XS MENS-S
                                                                                                            {% when 'S/M' %}
                                                                                                                WMNS-S MENS-M
                                                                                                            {% when 'M/L' %}
                                                                                                                WMNS-M MENS-L
                                                                                                            {% when 'L/XL' %}
                                                                                                                WMNS-L MENS-XL
                                                                                                            {% when 'XL/2XL' %}
                                                                                                                WMNS-XL MENS-2XL
                                                                                                            {% when '2XL/3XL' %}
                                                                                                                WMNS-2XL MENS-3XL
                                                                                                        {% endcase %}
                                                                                                    </li>
                                                                                                </div>
                                                                                            {% endif %}
                                                                                        {% endif %}
                                                                                    {% endfor %}
                                                                                </div>
																				<div class="size-chart"></div>
                                                                            </div>
                                                                            <div class="message-content"></div>
                                                                        {% endif %}
                                                                        {% form 'product', product, id:form_id, class:form_class %}
                                                                            <select name="id" id="ProductSelect-{{ section.id }}">
                                                                                {% for variant in product.variants %}
                                                                                    <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} {% unless variant.available %} disabled="disabled" {% endunless %} value="{{ variant.id }}" data-sku="{{ variant.sku }}">{{ variant.title }}
                                                                                        -
                                                                                        {% if variant.available %}
                                                                                            {{ variant.price | money_with_currency }}
