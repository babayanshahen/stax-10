                                                                                                <div class="continue-pre-content">PRE SALE</div>
                                                                                            </div>
                                                                                        {% endif %}
                                                                                        {% case variant.title %}
                                                                                            {% when 'xs' or 'XS' %}
                                                                                                XS
                                                                                            {% when 'small' or 'S' %}
                                                                                                S
                                                                                            {% when 'medium' or 'M' %}
                                                                                                M
                                                                                            {% when 'large'  or 'L' %}
                                                                                                L
                                                                                            {% when 'xl' or 'XL' %}
                                                                                                XL
                                                                                            {% when '2xl' or  '2XL' %}
                                                                                                2XL
                                                                                            {% when '3xl' or '3XL' %}
                                                                                                3XL
                                                                                      		{% else %}
																								{{ variant.title }}
                                                                                        {% endcase %}
                                                                                    </li>
                                                                                </div>
                                                                                {% else: %}
                                                                                {% form 'product', product, id:form_id, class:form_class %}
                                                                                    {% comment %}
