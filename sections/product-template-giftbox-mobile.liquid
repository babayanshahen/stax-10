                                                </nav>
                                                {% assign first_img = '' %}
                                                {% assign first_img_url = '' %}
                                                {% for image in product.images %}
                                                    {% assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                                                    {% if  forloop.index  == 1 %}
                                                        {% assign first_img = image %}
                                                        {% assign first_img_url = img_url %}
                                                    {% endif %}
                                                    <div style="width:49%">
                                                        <img class="grid-view-item__image lazyload {{ forloop.index }}" data-src="{{ image | img_url: '800x' }}" data-widths="[360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048, 4472]" data-aspectratio="{{ image.aspect_ratio }}" data-sizes="auto" src="" alt="{{ image.alt | escape }}"></div>
                                                    {% endfor %}
                                                </div>
                                            </div>
                                            {% if product.metafields.title.meta_title != blank %}
                                                <h1 class="bn-custom-text-redesigner">{{ product.metafields.title.meta_title }}</h1>
                                            {% else %}
                                                <h1 class="prod-title-desktop">{{ product.title }}</h1>
                                            {% endif %}
                                            <div class="product-details">
