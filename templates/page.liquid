{% include 'aaa_mem_helper' with 'page' %}<!-- /templates/page.liquid -->
 {% if  page.handle  == "bn_shipping" %}
    <style>
      .site-footer{
            display: none;
      }
      .bn-container{
            display: none;
      }
    </style>
{% endif %}
 {% if  page.handle  == "returns" %}
    <style>
      .site-footer{
            display: none;
      }
      .bn-container{
            display: none;
      }
    </style>
{% endif %}
<div class="page-width page-container">
  
  <header>
    <h1>{{ page.title }}</h1>
  </header>
  <div class="rte rte--indented-images">
    {{ page.content }}
  </div>
</div>
